{
  "env": {
    "es6": true,
    "node": true,
    "commonjs": true,
    "mongo": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "indent": [
      "error",
      "tab"
    ],
    "quotes": [
      "error",
      "double"
    ]
  }
}